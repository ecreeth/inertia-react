{
  "name": "@inertiajs/inertia-react",
  "version": "0.1.0",
  "license": "MIT",
  "description": "The React adapter for Inertia.js",
  "contributors": [
    "Jonathan Reinink <jonathan@reinink.ca>",
    "Sebastian De Deyne <sebastiandedeyne@gmail.com>"
  ],
  "source": "src/index.js",
  "main": "dist/index.js",
  "unpkg": "dist/index.umd.js",
  "typings": "index.d.ts",
  "scripts": {
    "build": "npm run build:cjs && npm run build:umd",
    "build:cjs": "microbundle --format cjs",
    "build:umd": "microbundle --format umd --name InertiaReact --globals react=React,inertia=Inertia",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@inertiajs/inertia": "^0.1.0",
    "@types/react": "^16.8.0",
    "eslint": "^5.15.3",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.6.0",
    "microbundle": "^0.11.0",
    "react": "^16.8.0"
  },
  "peerDependencies": {
    "@inertiajs/inertia": "^0.1.0",
    "react": "^16.8.0"
  }
}
